<?xml version="1.0"?>
<services xmlns="http://code.google.com/p/outfox">
  <service id="audio">
    <executable path="outfox.py">
      <arg value="audio" />
    </executable>
    <executable path="dist/mypython.exe">
      <arg value="audio" />
    </executable>
  </service>
  <service id="echo">
    <executable path="outfox.py">
      <arg value="echo" />
    </executable>
    <executable path="dist/mypython.exe">
      <arg value="echo" />
    </executable>
  </service>
  <service id="wiimote">
    <executable path="outfox.py">
      <arg value="wiimote" />
    </executable>
    <executable path="dist/mypython.exe">
      <arg value="wiimote" />
    </executable>
  </service>
</services>
